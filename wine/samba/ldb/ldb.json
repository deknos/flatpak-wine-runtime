{
    "name": "ldb",
    "config-opts": [
        "--disable-python",
        "--disable-rpath",
        "--disable-rpath-install",
        "--builtin-libraries=replace",
        "--with-modulesdir=/usr/lib/x86_64-linux-gnu/ldb/modules",
        "--with-privatelibdir=/usr/lib/x86_64-linux-gnu/ldb"
    ],
    "sources": [
        "sources.json"
    ],
    "cleanup": [
        "/bin",
        "/share/man"
    ],
    "modules": [
        "../popt/popt.json",
        "../lmdb/lmdb.json"
    ]
}
